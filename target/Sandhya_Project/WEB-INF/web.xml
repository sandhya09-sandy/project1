<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>Sandhya_Project</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sandhya_Project</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>new1</servlet-name>
    <servlet-class>com.project.Create</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>new1</servlet-name>
    <url-pattern>/createclass</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>new2</servlet-name>
    <servlet-class>com.project.Register</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>new2</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>new3</servlet-name>
    <servlet-class>com.project.Login</servlet-class>
  </servlet>

  <filter>
    <filter-name>AppFilter</filter-name>
    <filter-class>com.project.LoginFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>AppFilter</filter-name>
    <url-pattern>/loginuser</url-pattern>
  </filter-mapping>
  <servlet-mapping>
    <servlet-name>new3</servlet-name>
    <url-pattern>/loginuser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>new4</servlet-name>
    <servlet-class>com.project.Enroll</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>new4</servlet-name>
    <url-pattern>/enrollclass</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>new5</servlet-name>
    <servlet-class>com.project.DisplayClasses</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>new5</servlet-name>
    <url-pattern>/displayclasses</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>new6</servlet-name>
    <servlet-class>com.project.Assignment</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>new6</servlet-name>
    <url-pattern>/assignuser</url-pattern>
  </servlet-mapping>

</web-app>
